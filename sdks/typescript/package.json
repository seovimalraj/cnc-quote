{
  "name": "@frigate/typescript-sdk",
  "version": "1.0.0",
  "description": "Official TypeScript SDK for Frigate Manufacturing API",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "generate": "openapi-typescript-codegen --input ../../apps/api/src/docs/openapi.json --output ./src --client fetch",
    "generate:remote": "openapi-typescript-codegen --input https://api.frigate.ai/openapi.json --output ./src --client fetch",
    "build": "tsc",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "frigate",
    "manufacturing",
    "cnc",
    "quoting",
    "api",
    "sdk",
    "typescript"
  ],
  "author": "Frigate",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/frigate/typescript-sdk.git"
  },
  "dependencies": {
    "cross-fetch": "^4.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "openapi-typescript-codegen": "^0.27.0",
    "typescript": "^5.0.0"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  }
}
