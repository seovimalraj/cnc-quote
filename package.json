{
  "name": "cnc-quote",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "start": "node apps/api/dist/main.js",
    "dev": "turbo run dev",
  "lint": "turbo run lint",
  "dead-code": "pnpm -w ts-prune -g -p packages/shared/tsconfig.json && pnpm -w ts-prune -g -p apps/api/tsconfig.build.json && pnpm -w ts-prune -g -p apps/web/tsconfig.json",
    "clean": "turbo run clean",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "test": "turbo run test",
    "test:admin": "pnpm --filter @cnc-quote/web exec tsx --test lib/api/__tests__/admin.contract.test.ts",
    "deploy": "turbo run build && pnpm start",
    "type-check": "turbo run type-check",
    "prepare": "husky install",
    "clean:web": "cd apps/web && rm -rf .next out",
    "clean:api": "cd apps/api && rm -rf dist",
    "clean:all": "pnpm clean:web && pnpm clean:api && rm -rf node_modules && pnpm install",
    "qa": "node scripts/qa-runner.cjs",
    "qa:fixes": "cat QA_FIXES_SUMMARY.md",
    "qa:check-rls": "node scripts/check-rls.js",
    "qa:check-pricing": "node scripts/check-pricing.js",
    "qa:check-cad": "node scripts/check-cad.js",
    "qa:check-payment": "node scripts/check-payment.js",
    "qa:check-slos": "node scripts/check-slos.js",
    "qa:check-observability": "node scripts/check-observability.js",
  "qa:check-recalc": "tsx scripts/check-recalc.ts",
    "qa:check-supplier": "tsx scripts/check-supplier-sync.ts",
    "qa:test-storage-signed-url": "node scripts/test-storage-signed-url.js",
    "qa:bomb-leads": "node scripts/bomb-leads.js",
    "qa:ai-regression": "ts-node --esm scripts/qa-ai-regression.ts",
    "qa:seed-ollama-chaos": "ts-node --esm scripts/seed-ollama-chaos.ts",
    "qa:canary-shadow": "ts-node --esm scripts/canary-shadow-compare.ts",
    "audit:admin:surface": "tsx scripts/audit/admin-surface-map.ts",
    "audit:admin:flow": "tsx scripts/audit/admin-flow-wiring.ts",
    "audit:admin:api-trace": "tsx scripts/audit/admin-api-trace.ts",
    "audit:admin:dto-drift": "tsx scripts/audit/admin-dto-drift.ts",
    "audit:supplier:surface": "tsx scripts/audit/supplier-surface-map.ts",
    "audit:supplier:flow": "tsx scripts/audit/supplier-flow-wiring.ts",
    "audit:supplier:api-trace": "tsx scripts/audit/supplier-api-trace.ts",
    "audit:supplier:queue": "tsx scripts/audit/supplier-queue-check.ts",
    "audit:customer:surface": "tsx scripts/audit/customer-surface-map.ts",
    "audit:customer:flow": "tsx scripts/audit/customer-critical-flow-audit.ts",
    "audit:customer:api-trace": "tsx scripts/audit/customer-api-trace.ts",
    "audit:customer:dashboard": "tsx scripts/audit/customer-audit-dashboard.ts",
  "audit:customer:journey-validate": "tsx scripts/audit/customer-journey-validate.ts",
    "ai:train": "ts-node --esm scripts/ai/model-lifecycle.ts train",
    "ai:rollback": "ts-node --esm scripts/ai/model-lifecycle.ts rollback",
    "ai:bias-review": "ts-node --esm scripts/ai/model-lifecycle.ts bias-review"
  },
  "dependencies": {
    "@nestjs/axios": "^4.0.1",
    "@sentry/node": "^10.12.0",
    "@supabase/supabase-js": "^2.57.4",
    "@xenova/transformers": "^2.17.2",
    "axios": "^1.12.2",
    "file-type": "^21.0.0",
    "ollama": "^0.6.0",
    "onnxruntime-node": "^1.23.0",
    "posthog-node": "^5.8.4",
    "puppeteer": "^24.22.0",
    "zod": "^4.1.9"
  },
  "devDependencies": {
    "@types/busboy": "^1.5.4",
    "@types/node": "^24.5.2",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
  "@eslint/js": "^9.14.0",
    "eslint": "^9.35.0",
    "eslint-plugin-deprecation": "^3.0.0",
    "jest": "^29.7.0",
    "husky": "^8.0.3",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.3",
    "ts-node": "^10.9.2",
    "ts-morph": "^24.0.0",
    "tsx": "^4.19.1",
    "globby": "^14.0.2",
    "turbo": "^2.5.6",
    "ts-prune": "^0.10.3",
    "typescript": "^5.9.2",
    "globals": "^15.10.0"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "packageManager": "pnpm@10.16.1",
  "pnpm": {
    "overrides": {
      "minimist": "^1.2.8",
      "mkdirp": "^3.0.1",
      "qs": "^6.14.0",
      "lodash": "^4.17.21",
      "glob": "^10.0.0",
      "inflight": "^1.0.6"
    }
  }
}
