{
  "journeyId": "standard_quote_v1",
  "description": "Deterministic journey covering RFQ intake through checkout with expected API touchpoints.",
  "calls": [
    {
      "order": 1,
      "route": "/get-quote",
      "file": "apps/web/app/get-quote/page.tsx",
      "method": "GET",
      "url": "/api/customer/session",
      "expectedController": "CustomerSessionController.getSession"
    },
    {
      "order": 2,
      "route": "/instant-quote",
      "file": "apps/web/app/instant-quote/page.tsx",
      "method": "POST",
      "url": "/api/quotes/geometry",
      "expectedController": "QuotesGeometryController.createGeometry",
      "expectations": {
        "status": [202]
      }
    },
    {
      "order": 3,
      "route": "/instant-quote",
      "file": "apps/web/app/instant-quote/page.tsx",
      "method": "GET",
      "url": "/api/quotes/geometry/{geometryId}",
      "expectedController": "QuotesGeometryController.getGeometry",
      "expectations": {
        "status": [200]
      }
    },
    {
      "order": 4,
      "route": "/dfm-analysis",
      "file": "apps/web/app/dfm-analysis/page.tsx",
      "method": "GET",
      "url": "/api/dfm/reports/{quoteId}",
      "expectedController": "DfmReportsController.getReport",
      "expectations": {
        "status": [200]
      }
    },
    {
      "order": 5,
      "route": "/quotes",
      "file": "apps/web/app/quotes/page.tsx",
      "method": "POST",
      "url": "/api/quotes",
      "expectedController": "QuotesController.createQuote",
      "expectations": {
        "status": [201]
      }
    },
    {
      "order": 6,
      "route": "/secure-checkout",
      "file": "apps/web/app/secure-checkout/page.tsx",
      "method": "POST",
      "url": "/api/checkout/session",
      "expectedController": "CheckoutController.createSession",
      "expectations": {
        "status": [201]
      }
    }
  ]
}
