# Marketplace Backend Configuration
# Copy this file to .env.local and update the values

# Database (Supabase)
SUPABASE_URL=https://app.frigate.ai/db
SUPABASE_SERVICE_KEY=your_supabase_service_key_here
DATABASE_URL=postgresql://postgres:your_password@localhost:5432/cnc_quote

# MinIO Object Storage
MINIO_ENDPOINT=minio
MINIO_PORT=9000
MINIO_ACCESS_KEY=minioadmin
MINIO_ROOT_PASSWORD=minioadmin123
MINIO_SECRET_KEY=minioadmin123
MINIO_USE_SSL=false
MINIO_BUCKET_CAD_FILES=cad-files

# Redis
REDIS_URL=redis://redis:6379
REDIS_HOST=redis
REDIS_PORT=6379

# JWT
JWT_SECRET=your_jwt_secret_here

# API
NODE_ENV=production
PORT=3001
APP_URL=https://app.frigate.ai
ALLOWED_ORIGINS=https://app.frigate.ai,https://app.frigate.ai/api

# Next.js Web App
NEXT_PUBLIC_API_URL=https://app.frigate.ai/api
NEXT_PUBLIC_SUPABASE_URL=https://app.frigate.ai/db
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Email (Resend)
RESEND_API_KEY=your_resend_api_key_here
EMAIL_FROM=noreply@frigate.ai

# Optional: Sentry Error Tracking
SENTRY_DSN=your_sentry_dsn_here

# Optional: Analytics
POSTHOG_API_KEY=your_posthog_key_here
