{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "skipLibCheck": true,
    "noImplicitAny": false,
    "strict": false,
    "exactOptionalPropertyTypes": false,
    "noImplicitReturns": false,
    "noFallthroughCasesInSwitch": false,
    "noUncheckedIndexedAccess": false,
    "isolatedModules": false,
    "allowJs": true,
    "checkJs": false
  },
  "ts-node": {
    "transpileOnly": true,
    "ignoreDiagnostics": [
      1238, 1241, 1270, 1240, 1206, 1139, 1434, 1068, 1005, 1128
    ]
  }
}
