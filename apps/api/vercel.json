{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "framework": "node",
  "buildCommand": "cd ../.. && pnpm install && pnpm build",
  "outputDirectory": "dist",
  "ignoreCommand": "git diff HEAD^ HEAD --quiet ./",
  "installCommand": "pnpm install",
  "regions": ["iad1"],
  "functions": {
    "api/**/*.js": {
      "memory": 1024,
      "maxDuration": 60
    }
  }
}
