MODEL_GATEWAY_URL=https://model-gateway.local
MODEL_SERVICE_KEYSET={"keys":[{"kid":"dev","privateKey":"-----BEGIN PRIVATE KEY-----\n...\n-----END PRIVATE KEY-----"}]}
